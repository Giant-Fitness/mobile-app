{
    "expo": {
        "name": "LMC",
        "slug": "lmc-app",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": false,
            "userInterfaceStyle": "automatic",
            "bundleIdentifier": "com.GiantFitness.LMC",
            "minimimVersionIOS": "15.1",
            "infoPlist": {
                "UIRequiresFullScreen": true,
                "UIStatusBarStyle": "auto",
                "NSCameraUsageDescription": "This app needs access to the camera to scan QR codes.",
                "NSPhotoLibraryUsageDescription": "This app needs access to photos for profile pictures.",
                "NSUserNotificationsUsageDescription": "We'll send you notifications about your workouts and updates.",
                "UIRequiredDeviceCapabilities": ["armv7", "telephony"],
                "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "userInterfaceStyle": "automatic",
            "package": "com.GiantFitness.LMC",
            "minSdkVersion": 24,
            "permissions": ["NOTIFICATIONS", "RECEIVE_BOOT_COMPLETED", "VIBRATE", "CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "8f1030a1-b4d2-4668-9c53-b1e7cfbabf24"
            }
        },
        "owner": "hustleclub",
        "runtimeVersion": "0.0.1",
        "updates": {
            "url": "https://u.expo.dev/8f1030a1-b4d2-4668-9c53-b1e7cfbabf24"
        }
    }
}
