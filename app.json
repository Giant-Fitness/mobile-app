{
    "expo": {
        "name": "LMC",
        "slug": "lmc-app",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "giantfitness",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "googleServicesFile": "./GoogleService-Info.plist",
            "supportsTablet": false,
            "userInterfaceStyle": "automatic",
            "bundleIdentifier": "com.GiantFitness.LMC",
            "infoPlist": {
                "UIRequiresFullScreen": true,
                "UIStatusBarStyle": "auto",
                "NSCameraUsageDescription": "This app needs access to the camera to scan QR codes.",
                "NSPhotoLibraryUsageDescription": "This app needs access to photos for profile pictures.",
                "NSUserNotificationUsageDescription": "We'll send you notifications about your workouts and updates.",
                "UIRequiredDeviceCapabilities": ["arm64"],
                "ITSAppUsesNonExemptEncryption": false,
                "UIBackgroundModes": ["fetch", "remote-notification"]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "userInterfaceStyle": "automatic",
            "package": "com.GiantFitness.LMC",
            "permissions": ["NOTIFICATIONS", "RECEIVE_BOOT_COMPLETED", "VIBRATE", "CAMERA", "READ_MEDIA_IMAGES"],
            "intentFilters": [
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": {
                        "scheme": "giantfitness"
                    },
                    "category": ["BROWSABLE", "DEFAULT"]
                }
            ]
        },
        "plugins": ["expo-router", "expo-notifications", "expo-camera", "expo-font", "expo-secure-store", "expo-localization"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "8f1030a1-b4d2-4668-9c53-b1e7cfbabf24"
            }
        },
        "owner": "hustleclub",
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/8f1030a1-b4d2-4668-9c53-b1e7cfbabf24",
            "enabled": true,
            "fallbackToCacheTimeout": 0,
            "checkAutomatically": "ON_LOAD"
        }
    }
}
