{
    "cli": {
        "version": ">= 12.6.2",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "autoIncrement": true,
            "developmentClient": false,
            "distribution": "internal",
            "channel": "development",
            "env": {
                "APP_ENV": "development"
            },
            "ios": {
                "resourceClass": "medium",
                "buildConfiguration": "Release"
            },
            "android": {
                "resourceClass": "medium",
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "store",
            "channel": "preview",
            "autoIncrement": true,
            "env": {
                "APP_ENV": "preview"
            },
            "ios": {
                "resourceClass": "medium",
                "buildConfiguration": "Release"
            },
            "android": {
                "resourceClass": "medium",
                "buildType": "app-bundle"
            }
        },
        "production": {
            "channel": "production",
            "distribution": "store",
            "autoIncrement": true,
            "env": {
                "APP_ENV": "production"
            },
            "ios": {
                "resourceClass": "medium",
                "buildConfiguration": "Release"
            },
            "android": {
                "resourceClass": "medium",
                "buildType": "app-bundle"
            }
        }
    }
}
